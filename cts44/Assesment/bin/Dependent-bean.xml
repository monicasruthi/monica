<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   <bean id="addr" class="Four.Address">
   <constructor-arg value="indra naga"/>
   <constructor-arg value="gachibowli"/>
   <constructor-arg value="Hyderabad"/>
   </bean>
   <bean id="per" class="Four.Person" autowire="constructor">
   <constructor-arg value="sravanthi"/>
   <constructor-arg value="22"/>
   <constructor-arg>
   <ref bean="addr"/>
   </constructor-arg>
   </bean>
</beans>
